{
  "name": "@oclif/example-plugin-js",
  "description": "example dxcli plugin in javascript",
  "version": "1.0.2",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/oclif/example-plugin-js/issues",
  "dependencies": {
    "@oclif/command": "^1.2.22",
    "@oclif/config": "^1.3.47",
    "@oclif/errors": "^1.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.0.0",
    "@oclif/plugin-help": "^0.7.4",
    "@oclif/test": "^0.10.16",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-oclif": "^1.3.5",
    "globby": "^8.0.0",
    "mocha": "^5.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/oclif/example-plugin-js",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./src/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "oclif/example-plugin-js",
  "scripts": {
    "clean": "rm -f .oclif.manifest.json",
    "lint": "eslint .",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepublishOnly": "oclif-dev manifest && oclif-dev readme",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
