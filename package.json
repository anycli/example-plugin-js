{
  "name": "@anycli/example-plugin-js",
  "description": "example dxcli plugin in javascript",
  "version": "0.9.2",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./src/commands",
    "plugins": [
      "@anycli/plugin-help"
    ]
  },
  "bugs": "https://github.com/anycli/example-plugin-js/issues",
  "dependencies": {
    "@anycli/command": "^0.3.7",
    "cli-ux": "^3.3.13"
  },
  "devDependencies": {
    "@anycli/config": "^1.0.0",
    "@anycli/dev-cli": "^0.1.3",
    "@anycli/engine": "^0.3.1",
    "@anycli/plugin-help": "^0.4.6",
    "@anycli/test": "^0.10.3",
    "chai": "^4.1.2",
    "eslint": "^4.16.0",
    "eslint-config-anycli": "^1.3.1",
    "mocha": "^5.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".anycli.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/anycli/example-plugin-js",
  "keywords": [
    "anycli-plugin"
  ],
  "license": "MIT",
  "repository": "anycli/example-plugin-js",
  "scripts": {
    "lint": "eslint .",
    "postpublish": "rm .anycli.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "anycli-dev manifest -o .anycli.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.js\""
  }
}
